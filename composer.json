{
    "name": "ksfraser/amortizations-frontaccounting",
    "description": "FrontAccounting module for KSF Amortization system",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ksfraser/ksf_amortization_core.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ksfraser/html.git"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ksfraser/amortizations-core": "1.0.4",
        "ksfraser/html": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Ksfraser\\Amortizations\\FA\\": "src/",
            "Ksfraser\\Amortizations\\Repository\\": "Repository/"
        }
    },
    "scripts": {
        "test": "cd ../.. && composer test-fa"
    }
}
