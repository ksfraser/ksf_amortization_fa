{
    "name": "ksfraser/amortizations-frontaccounting",
    "description": "FrontAccounting module for KSF Amortization system",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "repositories": [
        {
            "type": "path",
            "url": "../..",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../vendor-src/ksfraser-html",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": ">=7.3",
        "ksfraser/amortizations-core": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Ksfraser\\Amortizations\\FA\\": "src/"
        }
    },
    "scripts": {
        "test": "cd ../.. && composer test-fa"
    }
}
